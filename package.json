{
  "name": "components-lib-1.0",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.4.1",
    "rc-slider": "^8.7.1",
    "react": "^16.12.0",
    "react-calendar": "^2.19.2",
    "react-datepicker": "^2.11.0",
    "react-dom": "^16.3.2",
    "react-outside-click-handler": "^1.3.0",
    "react-popper": "^1.3.7",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "storybook": "npm run build:helper-css && start-storybook -p 9009",
    "build:helper-css": "./node_modules/.bin/sass ./src/Theme/scss/index.scss:./src/Theme/index.css",
    "build:scss": "./node_modules/.bin/sass ./src/assets/scss/index.scss:./build/assets/index.css",
    "build:atom": "babel ./src/Components/Atom --out-dir ./build/components/Atom --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:molecules": "babel ./src/Components/Molecules --out-dir ./build/components/Molecules --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:organisms": "babel ./src/Components/Organisms --out-dir ./build/components/Organisms --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:utils": "babel ./src/Components/Utils --out-dir ./build/components/Utils --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:copy-files": "node ./scripts/copy-files.js",
    "build": "npm run build:atom && npm run build:molecules && npm run build:organisms && npm run build:utils && npm run build:scss && npm run build:copy-files",
    "test": "node scripts/test.js",
    "release": "npm run build && npm publish build --tag latest",
    "build-storybook": "build-storybook -c .storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-json-strings": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/cli": "^5.3.12",
    "@storybook/codemod": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@svgr/webpack": "^5.2.0",
    "autoprefixer": "^9.4.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "^25.1.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jscodeshift": "^0.6.4",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-cli": "^6.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^0.2.0",
    "react-dev-utils": "^7.0.1",
    "react-dropzone": "^8.0.3",
    "react-is": "^16.12.0",
    "resolve": "1.8.1",
    "sass": "^1.22.12",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "1.1.1",
    "webpack": "^4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  }
}
